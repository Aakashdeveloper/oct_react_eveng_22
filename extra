/////CSS//////
inline
styletag
stylesheet

inline>styletag>stylesheet

inline
{
    color:red;
    font-size:30px
}

styletag
{
    color:orange;
    border:2px solid red;
    background:gray
}

stylesheet
{
   background:white;
   font-size:40px;
   width:30px 
}

{
    color:red;
    font-size:30px;
    border:2px solid red;
    background:gray;
    width:30px 
}


